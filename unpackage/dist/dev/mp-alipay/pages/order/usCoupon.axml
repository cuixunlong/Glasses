<view class="coupon"><block a:if="{{couponList.length==0}}"><view class="empty"><u-image vue-id="9ef50744-1" src="/static/user_info/order_empty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">暂无可用优惠卷</view></view></block><view class="coupon_list"><view class="coupon_item" a:for="{{couponList}}" a:for-item="item" a:for-index="index" a:key="m_coupon_id"><view class="info"><view class="title">{{item.coupon_name}}</view><block a:if="{{item.coupon_type==0}}"><view class="type">品类限制：全场通用</view></block><block a:if="{{item.coupon_type==1}}"><view class="type">{{"品类限制："+item.sort_name}}</view></block><block a:if="{{item.coupon_type==2}}"><view class="type">{{"品类限制："+item.goods_name}}</view></block><view class="type">{{"有限期至："+item.end_time}}</view></view><view class="state"><view class="money"><view class="number">{{"￥"+item.coupon_money}}</view><view class="coupon_desc">{{item.desc}}</view><view data-event-opts="{{[['tap',[['choose',['$0'],[[['couponList','m_coupon_id',item.m_coupon_id]]]]]]]}}" class="condition" onTap="__e">立即使用</view></view></view></view></view></view>
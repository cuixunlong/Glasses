<view class="all_order"><view class="top"><view data-event-opts="{{[['tap',[['orderType',[index]]]]]}}" class="{{(('order_type')+' '+((index===itemindex)?'active':''))}}" onTap="__e" a:for="{{topList}}" a:for-item="item" a:for-index="index" a:key="index"><text class="title">{{item}}</text></view></view><block a:if="{{orderList.length==0}}"><view class="empty"><u-image vue-id="3c46aa78-1" src="/static/user_info/order_empty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">暂无订单</view></view></block><block a:else><view class="order_list"><view class="order" a:for="{{orderList}}" a:for-item="item" a:for-index="index" a:key="order_id"><navigator url="{{'orderDetails?order_id='+item.order_id}}" hover-class="none"><view class="info"><view class="infoImg"><u-image vue-id="{{'3c46aa78-2-'+index}}" src="{{item.order_item[0].goods_cover}}" mode onVueInit="__l"></u-image></view><view class="info_item"><view class="item_top"><view class="goods_title">{{item.order_item[0].goods_name}}</view><view class="number">{{"x"+item.order_item[0].goods_num}}</view></view><view class="item_bottom"><block a:if="{{item.need_time!='立即配送'}}"><view class="distribu"><view class="send_time">预计送达时间：</view><view>{{item.need_time+"前"}}</view></view></block><block a:else><view class="distribu">{{item.need_time}}</view></block><view class="number"><view>{{"共"+item.order_item[0].goods_num+"件"}}</view><view class="goods_price">{{"￥"+item.amount}}</view></view></view></view></view><view class="operation"><view class="time">{{item.create_time}}</view><view class="state">{{item.order_status_text}}</view><view class="order_detail">订单详情</view></view></navigator></view></view></block><block a:if="{{orderList.length!=0}}"><u-loadmore vue-id="3c46aa78-3" status="{{status}}" fontSize="20" color="#C2C5CC" icon-type="{{iconType}}" load-text="{{loadText}}" onVueInit="__l"></u-loadmore></block></view>
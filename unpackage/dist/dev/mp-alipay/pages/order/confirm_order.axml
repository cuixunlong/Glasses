<view class="confirm_order"><u-select vue-id="5f017f57-1" default-value="{{defaultTime}}" mode="mutil-column" list="{{timeList}}" confirm-color="#0E1E75" value="{{sendShow}}" data-event-opts="{{[['^confirm',[['sendConfirm']]],['^input',[['__set_model',['','sendShow','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-select><view class="order_item"><view class="order_info"><block a:if="{{orderInfo.address.length!=0}}"><view class="address_info"><view class="title">配送地址</view><view class="user_info"><view class="name">{{orderInfo.address.contacts}}</view><view class="number">{{orderInfo.address.contacts_phone}}</view></view><view data-event-opts="{{[['tap',[['toAddress',['$event']]]]]}}" class="address" onTap="__e"><view class="address_detail">{{orderInfo.address.address}}</view><view class="item_right"><u-icon vue-id="5f017f57-2" name="arrow-right" size="30" color="#8F9399" onVueInit="__l"></u-icon></view></view></view></block><block a:else><view data-event-opts="{{[['tap',[['toAddress',['$event']]]]]}}" class="empty_address" onTap="__e"><view class="iconfont icon_add icon-xinjain-copy"></view>请增加配送地址</view></block><view data-event-opts="{{[['tap',[['openSennd',['$event']]]]]}}" class="send_time" onTap="__e"><view class="send_top"><block a:if="{{sendTime=='立即送出'}}"><view class="title">配送时间</view></block><block a:else><view class="title">预约配送时间</view></block><view class="order_data">{{arriveDay}}</view></view><view class="send_state"><view>{{sendTime}}</view><block a:if="{{sendTime!='立即送出'}}"><view class="item_right"><u-icon vue-id="5f017f57-3" name="arrow-down" size="40" color="#0E1E75" onVueInit="__l"></u-icon></view></block></view></view><view class="goods_detail"><view class="title">订单详情</view><view class="goods_info"><view><image class="goods_img" src="{{orderInfo.goods.logo}}" mode="widthFix"></image></view><view class="detail_right"><view class="goods_title">{{orderInfo.goods.name}}</view><view class="goods_price"><view class="pirce">{{''+(orderInfo.TasteSelect.name==undefined?'':orderInfo.TasteSelect.name)+"("+orderInfo.goods.sku.specs_name+')'}}</view><view class="quantity">×1</view></view></view></view><block a:if="{{orderInfo.CandleSelect==undefined}}"><view class="goods_info"><view><image class="goods_img" src="{{orderInfo.CandleSelect.cover_img}}" mode="widthFix"></image></view><view class="detail_right"><view class="goods_title">{{orderInfo.CandleSelect.name}}</view><view class="goods_price"><view class="pirce">{{"￥"+orderInfo.CandleSelect.price}}</view><view class="quantity">×1</view></view></view></view></block><view class="goods_info"><view><image class="goods_img" src="../../static/user_info/plate.png" mode="widthFix"></image></view><view class="detail_right"><view class="goods_title">圆形餐盘</view><view class="goods_price"><view class="pirce">{{"￥"+orderInfo.TablewarePrice}}</view><view class="quantity">{{"×"+orderInfo.TablewareNum}}</view></view></view></view></view><view data-event-opts="{{[['tap',[['chooseInvoice',['$event']]]]]}}" class="invoice" onTap="__e"><u-select vue-id="5f017f57-4" default-value="{{defaultInvoice}}" list="{{list}}" confirm-color="#0E1E75" value="{{show}}" data-event-opts="{{[['^confirm',[['confirm']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-select><view class="invoice_title">发票</view><view>{{invoice}}</view><view class="item_right"><u-icon vue-id="5f017f57-5" name="arrow-right" size="30" color="#8F9399" onVueInit="__l"></u-icon></view></view><view class="serve_money"><view class="title">账号抵用金</view><view class="serve_number">{{"-￥"+orderInfo.discount_money+"元"}}</view><view data-event-opts="{{[['tap',[['cardCheck',['$event']]]]]}}" class="{{(('check_box')+' '+((checked==true)?'is_checked':''))}}" onTap="__e"></view></view><navigator url="remarks" hover-class="none"><view class="remarks"><view class="remarks_title">备注</view><view class="remarks_content">{{remark}}</view><view class="item_right"><u-icon vue-id="5f017f57-6" name="arrow-right" size="30" color="#8F9399" onVueInit="__l"></u-icon></view></view></navigator></view></view><view data-event-opts="{{[['tap',[['toCoupon',['$event']]]]]}}" class="coupon_item" onTap="__e"><view class="coupon"><view class="title">优惠券</view><block a:if="{{coupon!=''}}"><view>{{coupon+"元"}}</view></block><block a:else><view>{{couponNum}}</view></block><view class="item_right"><u-icon vue-id="5f017f57-7" name="arrow-right" size="30" color="#8F9399" onVueInit="__l"></u-icon></view></view></view><view class="btn_list"><view class="btn_item"><view class="number">{{"共"+number+"件，合计："}}</view><view class="price">{{"￥"+orderInfo.Allprice+"元"}}</view><view data-event-opts="{{[['tap',[['toPay',['$event']]]]]}}" class="toPay" onTap="__e">立即支付</view></view></view></view>
page {
  background-color: #F8F7FC;
}
.cake_page {
  background-color: #F8F7FC;
  padding: 0 15px;
  height: auto;
  overflow: hidden;
}
.cake_page .top_swiper {
  margin-top: 15px;
}
.cake_page .page_content {
  width: 100%;
  background: #fff;
  box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.12);
  margin-top: 15px;
  font-size: 12px;
  border-radius: 10px;
}
.cake_page .page_content .page_content_icons_list {
  padding: 13px;
  display: flex;
  justify-content: space-around;
}
.cake_page .page_content .page_content_icons_list .page_content_icons_item {
  text-align: center;
}
.cake_page .page_content .page_content_icons_list .page_content_icons_item .page_content_icon {
  width: 61px;
  height: 61px;
}
.cake_page .page_content .page_content_icons_list .page_content_icons_item .page_content_icon_title {
  margin-top: 7px;
  color: #999;
  font-size: 12px;
}
.cake_page .page_content .page_content_icons_list .page_content_icons_item .page_content_icon_info {
  margin-top: 18px;
  font-size: 10px;
  color: #7986B9;
}
.cake_page .index_sign {
  width: 100%;
  height: 40px;
  line-height: 40px;
  color: #ffffff;
  text-align: center;
  font-size: 14px;
  background-color: #28A9C7;
  border-radius: 10px;
  margin-top: 15px;
}
.cake_page .is_login {
  margin-top: 40px !important;
}
.cake_page .index_activity {
  height: auto;
  overflow: hidden;
  margin-top: 66px;
}
.cake_page .index_activity .index_activity_image {
  width: 100%;
  vertical-align: middle;
}
.cake_page .index_activity .index_activity_item {
  background-color: #FFFFFF;
  border-radius: 10px;
  margin-bottom: 16px;
}
.cake_page .index_activity .index_activity_item .index_activity_content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 7px 12px 8px 8px;
}
.cake_page .index_activity .index_activity_item .index_activity_content .index_activity_info {
  font-size: 12px;
  flex: 7;
}
.cake_page .index_activity .index_activity_item .index_activity_content .index_activity_info .index_activity_info_title {
  font-weight: bold;
  color: #141F61;
  white-space: nowrap;
  overflow: hidden;
  max-width: 300px;
  text-overflow: ellipsis;
}
.cake_page .index_activity .index_activity_item .index_activity_content .index_activity_info .index_activity_info_time {
  font-size: 10px;
  color: #999999;
  margin-top: 4px;
}
.cake_page .index_activity .index_activity_item .index_activity_icon {
  display: flex;
  align-items: center;
}
.cake_page .index_activity .index_activity_item .index_activity_icon image {
  height: 15px;
}


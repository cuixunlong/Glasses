<view class="invoice"><u-modal vue-id="04cab017-1" content="确定删除吗？" show-title="{{false}}" show-cancel-button="{{true}}" confirm-color="#0E1E75" value="{{delShow}}" data-event-opts="{{[['^confirm',[['sure']]],['^input',[['__set_model',['','delShow','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-modal><u-select vue-id="04cab017-2" default-value="{{defaultValue}}" list="{{list}}" confirm-color="rgba(14, 30, 117, 1)" value="{{show}}" data-event-opts="{{[['^confirm',[['confirm']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-select><block a:if="{{invoiceList.length==0}}"><view class="empty"><u-image vue-id="04cab017-3" src="/static/user_info/detail_emoty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">暂无发票信息</view></view></block><block a:else><view class="invoice_list padd_auto"><view class="inactive_item" a:for="{{invoiceList}}" a:for-item="item" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['chooseInvaoice',['$0',index],[[['invoiceList','id',item.id]]]]]]]}}" class="invoice_info" onTap="__e"><view class="type">{{item.invoice_type}}</view><view class="name">{{item.invoice_head}}</view></view><view class="btn_list"><view data-event-opts="{{[['tap',[['isDel',['$0'],[[['invoiceList','id',item.id]]]]]]]}}" class="btn" onTap="__e">删除</view><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['invoiceList','id',item.id]]]]]]]}}" class="btn" onTap="__e">编辑</view></view></view></view></block><view class="preservation h_auto bg_color"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="btn" onTap="__e"><u-icon vue-id="04cab017-4" name="plus" onVueInit="__l"></u-icon><text>增加发票抬头</text></view></view></view>
<view class="cake_sort"><view class="cake_sort_content"><scroll-view class="cake_sort_left" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['choose',['$0',index],[[['list','sid',item.sid]]]]]]]}}" class="{{(('cake_sort_left_item')+' '+((itemIndex===index)?'active':''))}}" onTap="__e" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="sid"><view hidden="{{!(itemIndex===index)}}" class="line"></view><view class="cake_sort_left_info">{{item.name}}</view></view></scroll-view><scroll-view class="cake_sort_right" scroll-y="true" show-scrollbar="{{false}}"><view class="right_box"><view class="cake_top_image"><u-image vue-id="330a51d6-1" src="{{list[itemIndex].logo}}" mode="widthFix" border-radius="10px" onVueInit="__l"></u-image></view><block a:if="{{goodsInfo.length==0}}"><u-loadmore vue-id="330a51d6-2" status="{{status}}" load-text="{{loadText}}" color="#C2C5CC" fontSize="20" margin-top="30" onVueInit="__l"></u-loadmore></block><view class="cake_sort_right_list"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['goodsInfo','goods_id',item.goods_id]]]]]]]}}" class="cake_sort_right_item" onTap="__e" a:for="{{goodsInfo}}" a:for-item="item" a:for-index="index" a:key="goods_id"><view class="right_item_image"><u-image vue-id="{{'330a51d6-3-'+index}}" src="{{item.logo}}" mode border-radius="20" onVueInit="__l"></u-image></view><view class="item_text"><view class="good_info"><view class="title">{{item.name}}</view><u-parse class="info" vue-id="{{'330a51d6-4-'+index}}" html="{{item.title}}" onVueInit="__l"></u-parse></view><view class="price"><view class="price_number">{{"￥"+item.price+"起"}}</view><image src="../../static/sort/jia.png" mode="widthFix"></image></view></view></view></view></view></scroll-view></view></view>
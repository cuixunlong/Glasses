<view class="administration"><block a:if="{{addressList.length===0}}"><view class="empty"><u-image vue-id="84fa100e-1" src="/static/user_info/address_empty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">没有收货地址</view></view></block><u-modal vue-id="84fa100e-2" content="确定删除吗" show-title="{{false}}" show-cancel-button="{{true}}" confirm-color="#0E1E75" value="{{delShow}}" data-event-opts="{{[['^confirm',[['sure']]],['^input',[['__set_model',['','delShow','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-modal><block a:if="{{addressList.length!=0}}"><view class="address_list .padd_auto"><view class="address_item" a:for="{{addressList}}" a:for-item="item" a:for-index="index" a:key="address_id"><view class="address"><view data-event-opts="{{[['tap',[['chooseAdd',['$0',index],[[['addressList','address_id',item.address_id]]]]]]]}}" class="top_info" onTap="__e"><view class="name_num"><text class="name">{{item.contacts}}</text><text class="number">{{item.contacts_phone}}</text></view><view class="detailed_address"><text>{{item.address}}</text></view></view><view class="btn_list"><view data-event-opts="{{[['tap',[['isDefault',['$0'],[[['addressList','address_id',item.address_id]]]]]]]}}" class="left_check" onTap="__e"><view class="{{(('check_box')+' '+((item.is_default==1)?'is_check':''))}}"></view><view class="is_default">设为默认</view></view><view class="right_btn"><view data-event-opts="{{[['tap',[['deleteI',['$0'],[[['addressList','address_id',item.address_id]]]]]]]}}" class="btn" onTap="__e">删除</view><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['addressList','address_id',item.address_id]]]]]]]}}" class="btn" onTap="__e">编辑</view></view></view></view></view></view></block><view class="preservation h_auto bg_btn"><view data-event-opts="{{[['tap',[['newAddress',['$event']]]]]}}" class="btn" onTap="__e"><u-icon vue-id="84fa100e-3" name="plus" onVueInit="__l"></u-icon><text>新建收货地址</text></view></view></view>
<view class="detailed"><block a:if="{{detailedList.length==0}}"><view class="empty"><u-image vue-id="60deb266-1" src="/static/user_info/detail_emoty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">暂时没有明细</view></view></block><view class="detailed_list"><view class="detailed_item" a:for="{{detailedList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="left"><view class="title">{{item.intro}}</view><view class="time">{{item.create_time}}</view></view><view class="right"><view hidden="{{!(item.type=='sub')}}" class="pay">{{"-"+item.money}}</view><view hidden="{{!(item.type=='add')}}" class="recharge">{{"+"+item.money}}</view></view></view></view><block a:if="{{detailedList.length!=0}}"><u-loadmore vue-id="60deb266-2" status="{{status}}" fontSize="24" margin-top="30" color="#C2C5CC" icon-type="{{iconType}}" load-text="{{loadText}}" onVueInit="__l"></u-loadmore></block></view>
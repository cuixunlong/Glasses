<view class="coupon"><view class="coupon_type"><view data-event-opts="{{[['tap',[['choose',[index]]]]]}}" class="{{(('coupon_type_item')+' '+((itemindex===index)?'active':''))}}" onTap="__e" a:for="{{couponType}}" a:for-item="item" a:for-index="index" a:key="index"><view class="title">{{item}}</view><view hidden="{{!(itemindex===index)}}" class="line"></view></view></view><block a:if="{{couponList.length==0}}"><view class="empty"><u-image vue-id="4236b530-1" src="/static/user_info/order_empty.png" mode="widthFix" onVueInit="__l"></u-image><view class="empty_title">暂无优惠卷信息</view></view></block><view class="coupon_list"><view class="{{(('coupon_item')+' '+((item.status!=0)?'invalid_coupon':''))}}" a:for="{{couponList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="info"><view class="title">{{item.coupon_name}}</view><block a:if="{{item.coupon_type==0}}"><view class="type">品类限制：全场通用</view></block><block a:if="{{item.coupon_type==1}}"><view class="type">{{"品类限制："+item.sort_name}}</view></block><block a:if="{{item.coupon_type==2}}"><view class="type">{{"指定商品："+item.goods_name}}</view></block><view class="type">{{"有限期至："+item.end_time}}</view></view><view class="state"><view class="money"><view class="number">{{"￥"+item.coupon_money}}</view><view class="coupon_desc">{{item.desc}}</view><block a:if="{{item.status==0}}"><view data-event-opts="{{[['tap',[['toIndex',['$0'],[[['couponList','',index]]]]]]]}}" class="condition" onTap="__e">立即使用</view></block><block a:if="{{item.status==1}}"><view class="condition">已使用</view></block><block a:if="{{item.status==2}}"><view class="condition">已失效</view></block></view></view></view></view></view>
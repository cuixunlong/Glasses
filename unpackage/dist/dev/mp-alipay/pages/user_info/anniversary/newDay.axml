<view class="new_day"><view class="day_item"><view class="day_title"><u-input onInput="__e" vue-id="661b8b8a-1" placeholder="请输入纪念日标题" type="{{type}}" border="{{border}}" clearable="{{false}}" maxlength="20" value="{{from.memorial_title}}" data-event-opts="{{[['^input',[['__set_model',['$0','memorial_title','$event',[]],['from']]]]]}}" onVueInit="__l"></u-input></view><view data-event-opts="{{[['tap',[['getTime',['$event']]]]]}}" class="time" onTap="__e"><view class="title">时间</view><view class="choose">{{from.memorial_day}}</view><u-icon vue-id="661b8b8a-2" name="arrow-right" onVueInit="__l"></u-icon><u-picker vue-id="661b8b8a-3" mode="time" default-time="{{defaulTime}}" end-year="{{endYear}}" confirm-color="#0E1E75" value="{{timeShow}}" data-event-opts="{{[['^confirm',[['getData']]],['^input',[['__set_model',['','timeShow','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-picker></view><view data-event-opts="{{[['tap',[['getRemind',['$event']]]]]}}" class="time" onTap="__e"><view class="title">提醒</view><view class="choose">{{remind}}</view><u-icon vue-id="661b8b8a-4" name="arrow-right" onVueInit="__l"></u-icon><u-select vue-id="661b8b8a-5" default-value="{{defaultValue}}" list="{{remindList}}" confirm-color="#0E1E75" value="{{remindShow}}" data-event-opts="{{[['^confirm',[['changeRemind']]],['^input',[['__set_model',['','remindShow','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></u-select></view></view><view class="preservation h_auto"><view data-event-opts="{{[['tap',[['saveBtn',['$event']]]]]}}" class="btn" onTap="__e">保存</view></view></view>
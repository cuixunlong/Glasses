<view class="my_info"><block a:if="{{infoList.length==0}}"><u-empty vue-id="781b715c-1" text="消息列表为空" iconSize="130px" marginTop="200" mode="message" onVueInit="__l"></u-empty></block><block a:else><view class="info_list"><view class="info_item" a:for="{{infoList}}" a:for-item="item" a:for-index="index" a:key="msg_id"><view class="create_time"><text class="time">{{item.create_time}}</text></view><view class="uinfo_state"><view class="state_title">{{item.title}}</view><view class="state_detail">{{item.content}}</view></view></view><block a:if="{{infoList.length!=0}}"><u-loadmore vue-id="781b715c-2" status="{{status}}" icon-type="{{iconType}}" fontSize="20" color="#C2C5CC" margin-top="24" load-text="{{loadText}}" onVueInit="__l"></u-loadmore></block></view></block></view>
<view class="invita"><block a:if="{{invitaList.length==0}}"><u-empty vue-id="7ab5d1b1-1" text="还没有邀请好友" mode="history" margin-top="200" iconSize="130px" onVueInit="__l"></u-empty></block><block a:else><view class="invita_list"><view class="invita_item" a:for="{{invitaList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.face!=''}}"><u-image vue-id="{{'7ab5d1b1-2-'+index}}" src="{{item.face}}" mode="widthFix" shape="circle" onVueInit="__l"></u-image></block><view class="invita_info"><view class="number">{{item.mobile}}</view><view class="time">{{item.create_time}}</view></view><view class="coupon">{{item.coupon}}</view></view></view></block><block a:if="{{invitaList.length!=0}}"><u-loadmore vue-id="7ab5d1b1-3" status="{{status}}" font-size="20" color="#C2C5CC" icon-type="{{iconType}}" load-text="{{loadText}}" onVueInit="__l"></u-loadmore></block></view>
.cars_detail .cars_item {
  padding: 0 15px;
  margin-top: 27px;
}
.cars_detail .cars_item .cars_cover {
  width: 100%;
}
.cars_detail .cars_item .cars_cover .cars_image {
  width: 100%;
  display: block;
}
.cars_detail .cars_item .detail {
  margin-top: 24px;
  color: #919499;
  font-size: 10px;
}
.cars_detail .cars_item .detail .title {
  color: #303133;
  font-size: 16px;
  text-align: center;
  font-weight: bold;
}
.cars_detail .cars_item .detail .content {
  border: 1px solid #C2C6CC;
  box-sizing: border-box;
  border-radius: 10px;
  margin-top: 12px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 14px 67px 14px 24px;
}
.cars_detail .cars_item .detail .content .content_left {
  text-align: center;
}
.cars_detail .cars_item .detail .content .content_left .money {
  color: #0E1E75;
  font-size: 28px;
}
.cars_detail .btn_list {
  position: fixed;
  width: 100%;
  height: auto;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #FFFFFF;
  padding: 10px 15px;
  height: calc(48px+constant(safe-area-inset-bottom));
  height: calc(48px + env(safe-area-inset-bottom));
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.cars_detail .btn_list .total_price {
  color: #F16464;
  font-size: 20px;
}
.cars_detail .btn_list .toPay {
  color: #FFFFFF;
  font-size: 14px;
  padding: 8px 34px;
  background-color: #0E1E75;
  border-radius: 10px;
}


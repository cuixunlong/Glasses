.navigation {
  width: 100%;
  display: flex;
  border: solid 2rpx #f2f2f2;
  background-color: #ffffff;
  padding: 16rpx 0;
  border-bottom: 0;
}
.navigation .left {
  display: flex;
  font-size: 20rpx;
}
.navigation .left .item {
  margin: 0 30rpx;
}
.navigation .left .item.car {
  text-align: center;
  position: relative;
}
.navigation .left .item.car .car-num {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.navigation .right {
  display: flex;
  font-size: 28rpx;
  align-items: center;
}
.navigation .right .btn {
  line-height: 66rpx;
  padding: 0 30rpx;
  border-radius: 36rpx;
  color: #ffffff;
}
.navigation .right .cart {
  background-color: #ed3f14;
  margin-right: 30rpx;
}
.navigation .right .buy {
  background-color: #ff7900;
}
page {
  background-color: #FFFFFF;
}
.detail {
  background-color: #FFFFFF;
  overflow: hidden;
}
.detail .top_image {
  position: relative;
}
.detail .top_image .topswiper {
  height: 375px;
  overflow: hidden;
}
.detail .top_image .topswiper image {
  width: 100%;
}
.detail .top_image .left_back {
  position: absolute;
  left: 20px;
}
.detail .top_image .left_back .top_icon {
  width: 28px;
}
.detail .goods_details {
  background-color: #FFFFFF;
  height: auto;
  overflow: hidden;
  padding-bottom: calc((48px + env(safe-area-inset-bottom)) + (env(safe-area-inset-bottom)));
}
.detail .goods_describe {
  background-color: #FFFFFF;
  padding: 0 25px;
  padding-bottom: 10px;
  padding-top: 20px;
  letter-spacing: 0.1em;
}
.detail .goods_describe .goods_price {
  font-weight: bold;
  font-size: 44rpx;
  color: #FF7459;
  line-height: 36rpx;
  margin-bottom: 40rpx;
}
.detail .goods_describe .goods_title {
  font-size: 16px;
  max-width: 345px;
  color: #333333;
  margin-bottom: 10rpx;
}
.detail .goods_describe .goods_info {
  font-size: 24rpx;
  color: #999999;
  line-height: 36rpx;
}
.detail .botton {

page {
  background-color: #F3F4F5;
}
.jinian_day .day_list {
  padding: 0 15px;
}
.jinian_day .day_list .day_item:last-child {
  margin-bottom: 16px;
}
.jinian_day .day_list .day_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12px;
  margin-top: 16px;
  color: #000000;
  font-size: 16px;
  background: #FFFFFF;
  box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.08);
  border-radius: 8px;
}
.jinian_day .day_list .day_item .bnt_list {
  display: flex;
}
.jinian_day .day_list .day_item .bnt_list .btn:first-child {
  margin-right: 12px;
}
.jinian_day .day_list .day_item .bnt_list .btn {
  width: 52px;
  height: 24px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #FFFFFF;
  color: #666666;
  font-size: 14px;
  border: 1px solid #666666;
  border-radius: 4px;
}
.jinian_day .day_list .day_item .day_time {
  color: #919499;
  font-size: 12px;
  margin-top: 8px;
}
.jinian_day .day_list .day_item .day_title {
  font-weight: bold;
}


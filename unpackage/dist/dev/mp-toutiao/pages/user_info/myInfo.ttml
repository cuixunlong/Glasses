<view class="my_info"><block tt:if="{{infoList.length==0}}"><u-empty vue-id="78240565-1" text="消息列表为空" iconSize="130px" marginTop="200" mode="message" bind:__l="__l"></u-empty></block><block tt:else><view class="info_list"><block tt:for="{{infoList}}" tt:for-item="item" tt:for-index="index" tt:key="msg_id"><view class="info_item"><view class="create_time"><text class="time">{{item.create_time}}</text></view><view class="uinfo_state"><view class="state_title">{{item.title}}</view><view class="state_detail">{{item.content}}</view></view></view></block><block tt:if="{{infoList.length!=0}}"><u-loadmore vue-id="78240565-2" status="{{status}}" icon-type="{{iconType}}" fontSize="20" color="#C2C5CC" margin-top="24" load-text="{{loadText}}" bind:__l="__l"></u-loadmore></block></view></block></view>
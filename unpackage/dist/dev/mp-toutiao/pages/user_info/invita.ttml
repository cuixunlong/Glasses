<view class="invita"><block tt:if="{{invitaList.length==0}}"><u-empty vue-id="7abe65ba-1" text="还没有邀请好友" mode="history" margin-top="200" iconSize="130px" bind:__l="__l"></u-empty></block><block tt:else><view class="invita_list"><block tt:for="{{invitaList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="invita_item"><block tt:if="{{item.face!=''}}"><u-image vue-id="{{'7abe65ba-2-'+index}}" src="{{item.face}}" mode="widthFix" shape="circle" bind:__l="__l"></u-image></block><view class="invita_info"><view class="number">{{item.mobile}}</view><view class="time">{{item.create_time}}</view></view><view class="coupon">{{item.coupon}}</view></view></block></view></block><block tt:if="{{invitaList.length!=0}}"><u-loadmore vue-id="7abe65ba-3" status="{{status}}" font-size="20" color="#C2C5CC" icon-type="{{iconType}}" load-text="{{loadText}}" bind:__l="__l"></u-loadmore></block></view>
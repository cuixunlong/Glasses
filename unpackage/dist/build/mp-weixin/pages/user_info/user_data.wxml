<view class="user_data"><view class="user_data_info"><view class="user_item"><view class="user_item_head"><u-image class="head_image" vue-id="0bb8d2fe-1" src="{{userInfo.face}}" mode="widthFix" shape="circle" bind:__l="__l"></u-image></view></view><view class="user_item"><u-modal vue-id="0bb8d2fe-2" title="修改昵称" show-cancel-button="{{true}}" width="90%" confirm-color="#0E1E75" value="{{nameshow}}" data-event-opts="{{[['^confirm',[['changeName']]],['^input',[['__set_model',['','nameshow','$event',[]]]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l" vue-slots="{{['default']}}"><input class="name_input" type="text" value="{{newName}}" maxlength="15" data-event-opts="{{[['input',[['__set_model',['','newName','$event',[]]]]]]}}" bindinput="__e"/></u-modal><u-cell-item vue-id="0bb8d2fe-3" title="昵称" value="{{userInfo.nickname}}" arrow="{{false}}" hover-class="none" data-event-opts="{{[['^click',[['openname']]]]}}" bind:click="__e" bind:__l="__l"></u-cell-item></view><view class="user_item"><u-cell-item vue-id="0bb8d2fe-4" title="电话" value="{{userInfo.mobile}}" arrow="{{false}}" hover-class="none" bind:__l="__l"></u-cell-item></view><view class="user_item"><u-picker vue-id="0bb8d2fe-5" mode="time" default-time="{{defaultTime}}" end-year="{{endYear}}" confirm-color="#0E1E75" value="{{labshow}}" data-event-opts="{{[['^confirm',[['getData']]],['^input',[['__set_model',['','labshow','$event',[]]]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l"></u-picker><u-cell-item vue-id="0bb8d2fe-6" title="生日" value="{{userInfo.birthday}}" arrow="{{true}}" arrow-direction="down" hover-class="none" confirm-color="#0E1E75" data-event-opts="{{[['^click',[['openlab']]]]}}" bind:click="__e" bind:__l="__l"></u-cell-item></view><view class="user_item"><u-select vue-id="0bb8d2fe-7" default-value="{{defaultValue}}" list="{{sexList}}" confirm-color="#0E1E75" value="{{sexShow}}" data-event-opts="{{[['^confirm',[['getSex']]],['^input',[['__set_model',['','sexShow','$event',[]]]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l"></u-select><u-cell-item vue-id="0bb8d2fe-8" title="性别" value="{{userInfo.sex}}" arrow="{{true}}" arrow-direction="down" hover-class="none" data-event-opts="{{[['^click',[['openSex']]]]}}" bind:click="__e" bind:__l="__l"></u-cell-item></view><view class="user_item"><u-cell-item vue-id="0bb8d2fe-9" title="账户抵用金" value="{{userInfo.voucher_money}}" arrow="{{false}}" hover-class="none" bind:__l="__l"></u-cell-item></view></view><view data-event-opts="{{[['tap',[['outSign',['$event']]]]]}}" class="outSign" bindtap="__e">退出登录</view><u-modal vue-id="0bb8d2fe-10" content="确认退出吗" title-show="{{false}}" show-cancel-button="{{true}}" confirm-text="退出" cancel-text="取消" confirm-color="#0E1E75" value="{{show}}" data-event-opts="{{[['^confirm',[['outConfirm']]],['^cancel',[['outCancel']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" bind:confirm="__e" bind:cancel="__e" bind:input="__e" bind:__l="__l"></u-modal></view>